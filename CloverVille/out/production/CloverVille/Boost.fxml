<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="10"
      alignment="CENTER"
      xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="C.BoostController">

    <TextField fx:id="boostField" promptText="Boost name"/>

    <Button text="Add"
            onAction="#addBoost"/>

    <ListView fx:id="boostListView"/>

    <Button text="Delete"
            onAction="#deleteBoost"/>

    <Button text="Back"
            onAction="#goBackToMain"/>

</VBox>
